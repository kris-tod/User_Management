{
  "type": "module",
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.29",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^5.0.0-beta.1",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.2",
    "sequelize": "^6.32.1",
    "validator": "^13.9.0",
    "winston": "^3.10.0"
  },
  "scripts": {
    "start": "node src/server/index.js",
    "lint": "eslint -c *.js"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "import/prefer-default-export": [
        "off"
      ],
      "import/extensions": [
        "off"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "max-classes-per-file": [
        "off"
      ]
    }
  }
}
